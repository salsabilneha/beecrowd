#include<stdio.h>
int main()
{
int ihours,iminutes,fhours,fminutes,initial,final,duration,hours,minutes;
scanf("%d%d%d%d",&ihours,&iminutes,&fhours,&fminutes);
initial=(ihours*60)+iminutes;
final=(fhours*60)+fminutes;
if(initial>=final)
{
final=final+(24*60);
}
duration=final-initial;
hours=duration/60;
minutes=duration%60;
printf("O JOGO DUROU %d HORA(S) E %d MINUTO(S)\n",hours,minutes);
return 0;
}
